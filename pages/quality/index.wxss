/* pages/quality/index.wxss */
.header {
  background: rgb(4, 29, 59);
}

.aqiText {
  color: #fff;
  font-size: 30rpx;
  margin-bottom: 20rpx;
}

.aqiVal {
  font-size: 100rpx;
  color: #fff;
}

.time {
  float: right;
  font-size: 25rpx;
  color: #fff;
}

.primary {
  font-size: 30rpx;
  color: #fff;
  background: #1E334E;
  text-align: center;
  padding: 12rpx 0;
  /* position: relative; */
}

.pm10,
.pm2p5,
.no2,
.so2,
.co,
.o3 {

  margin-bottom: 5rpx;
}

.pm10::after,
.pm2p5::after,
.no2::after,
.so2::after,
.co::after,
.o3::after {
  content: '';
  display: inline-block;
  width: 15rpx;
  height: 15rpx;
  border-radius: 50%;
  position: absolute;
  margin-left: 10rpx;
}

.pm10::after {
  background-color: darkgoldenrod;
}

.pm2p5::after {
  background-color: red;
}

.no2::after,
.so2::after,
.co::after,
.o3::after {
  background-color: rgb(13, 216, 40);
}

.nowCol {
  border-right: 1px solid rgba(255, 255, 255, .8);
}

.nowCol:last-child {
  border: none
}

.card {
  background: rgb(4, 29, 59);
  padding-top: 20rpx;
}
.contentOne {
  background: #1E334E;
}
.contentText {
  margin-left: 20rpx;
  font-size: 35rpx;
  font-weight: bold;
}